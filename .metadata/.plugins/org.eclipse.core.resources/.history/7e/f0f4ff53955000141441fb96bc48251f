var builder = function(useable){
	//Main Window
	var win = Ti.UI.createWindow({
	backgroundColor: "#000"
});
	var title = Ti.UI.createLabel({
		top: "0dp",
		backgroundColor: "#ccc",
		text: "Updated Space News"
	});
	//Table View
	var tableView = Ti.UI.createTableView({
		top: "5%",
		data: useable
	});








win.add(title);
win.add(tableView);
win.open();
};

exports.builder = builder;

